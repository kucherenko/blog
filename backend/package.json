{
  "name": "telegram-bot",
  "version": "1.0.0",
  "description": "Telegram Bot to save links with descriptions",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@nestjs/common": "latest",
    "@nestjs/core": "latest",
    "telegraf": "latest",
    "typeorm": "latest",
    "reflect-metadata": "latest"
  },
  "devDependencies": {
    "@nestjs/cli": "latest",
    "typescript": "latest",
    "ts-node": "latest",
    "tsconfig-paths": "latest"
  }
}